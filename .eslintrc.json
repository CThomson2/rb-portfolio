{
  "extends": "next/core-web-vitals",
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": "error",
    "import/case-sensitivity": "error"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.json"
      },
      "@typescript-eslint/parser": [".ts", ".tsx", ".js", ".jsx"]
    }
  }
}
